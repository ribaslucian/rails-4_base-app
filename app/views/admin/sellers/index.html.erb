<p id="notice"><%= notice %></p>

<h1>Listing Admin Sellers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Corporate</th>
      <th>Cnpj</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_sellers.each do |admin_seller| %>
      <tr>
        <td><%= admin_seller.name %></td>
        <td><%= admin_seller.corporate %></td>
        <td><%= admin_seller.cnpj %></td>
        <td><%= link_to 'Show', admin_seller %></td>
        <td><%= link_to 'Edit', edit_admin_seller_path(admin_seller) %></td>
        <td><%= link_to 'Destroy', admin_seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_admin_seller_path %>
